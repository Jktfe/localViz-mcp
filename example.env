# Fooocus API Configuration
FOOOCUS_API_URL=http://127.0.0.1:8888
FOOOCUS_API_PORT=8888

# Performance and Timeout Settings
FOOOCUS_API_TIMEOUT=30000  # Timeout in ms for API requests
POLLING_INTERVAL=2000      # Interval in ms for checking job status

# Logging Configuration
# Allowed values: debug, info, warn, error
LOG_LEVEL=info             # Set to debug for detailed logs

# Fooocus API Paths
# Replace with your actual paths to Fooocus and Fooocus-API installations
FOOOCUS_API_PATH=/path/to/your/Fooocus-API
FOOOCUS_PATH=/path/to/your/Fooocus

# Output Path for Generated Images
# Replace with your preferred output directory
OUTPUT_DIR=./outputs

# Default Generation Settings
DEFAULT_PERFORMANCE=Quality
DEFAULT_ASPECT_RATIO=1152*896
DEFAULT_STYLE=Fooocus V2
DEFAULT_IMAGE_FORMAT=png

# Rate Limiting
MAX_CONCURRENT_JOBS=3
