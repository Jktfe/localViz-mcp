# Fooocus API Configuration
FOOOCUS_API_URL=http://127.0.0.1:8888
FOOOCUS_API_PORT=8888

# Performance and Timeout Settings
API_TIMEOUT=10000          # Timeout in ms for API requests
POLLING_INTERVAL=2000      # Interval in ms for checking job status

# Logging Configuration
# Allowed values: debug, info, warn, error
LOG_LEVEL=info             # Set to debug for detailed logs
LOG_FILE=localviz.log      # Log file path (relative or absolute)

# V1 Server Settings
MAX_CONCURRENT_JOBS=3      # Maximum number of concurrent generation jobs
CREATE_SUBDIRS=true        # Whether to create subdirectories for each generation
CREATE_METADATA=true       # Whether to create metadata files for generated images

# API Management Settings
MANAGE_API=true            # Whether to automatically manage Fooocus API
API_SHUTDOWN_TIMEOUT=300000 # Shutdown API after 5 minutes of inactivity

# Fooocus API Paths
# Replace with your actual paths to Fooocus and Fooocus-API installations
FOOOCUS_API_PATH=/path/to/your/Fooocus-API
FOOOCUS_PATH=/path/to/your/Fooocus

# Output Path for Generated Images
# Replace with your preferred output directory
OUTPUT_DIR=./outputs
